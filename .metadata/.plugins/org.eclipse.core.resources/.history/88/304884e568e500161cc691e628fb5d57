package model.data.components;

import java.awt.Point;
import java.io.Serializable;

@SuppressWarnings("serial")
public class Box extends Components implements Serializable {

	public Box() {}
	public Box(Point position) { super(position); }

	@Override
	public ComponentsType getWorldObjectType() {
		return ComponentsType.BOX;
	}

	@Override
	public char getObjRep() {
		return '@';
	}

	@Override
	public boolean onTarget() {
		return onTarget;
	}
	
}
