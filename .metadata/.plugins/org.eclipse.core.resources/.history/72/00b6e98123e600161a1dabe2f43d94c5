package model.data.component;

import java.awt.*;
import java.io.Serializable;

@SuppressWarnings("serial")
public class Player extends Component implements Serializable{

	public Player() {}
	public Player(Point position) {	super(position); }
	
	@Override
	public ComponentType getWorldObjectType() {return ComponentType.PLAYER;}

	@Override
	public char getObjRep() {return 'A';}

	@Override
	public boolean onTarget() {return onTarget;}

}
